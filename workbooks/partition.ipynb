    "def partition(n, k):\n",
    "    if n == 0 or k == 1:\n",
    "        return 1\n",
    "    \n",
    "    if n == 1:\n",
    "        return k\n",
    "    \n",
    "    sum = 0\n",
    "    for i in range(0, n+1): # from [0,n] inclusive\n",
    "        sum += partition(i, k-1)\n",
    "    \n",
    "    return sum"
   ]
  },
